<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application

        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/baseline_account_circle_24"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Hobies_Viajes"
        tools:targetApi="31">
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:theme="@style/Theme.Hobies_Viajes">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".PestanasActivity"
            android:exported="false"
            android:theme="@style/Theme.Hobies_Viajes">
        </activity>


        <activity
            android:name=".AnadirHobbiesActivity"
            android:exported="false"
            android:theme="@style/Theme.Hobies_Viajes">
        </activity>
        <activity
            android:name=".AnadirInformacionActivity"
            android:exported="false"
            android:theme="@style/Theme.Hobies_Viajes">
        </activity>
        <activity
            android:name=".AnadirNuevoViajesActivity"
            android:exported="false"
            android:theme="@style/Theme.Hobies_Viajes">
        </activity>


    </application>

</manifest>